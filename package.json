    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "express-session": "^1.16.2",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react-router-dom": "^5.0.1",
    "sequelize": "^4.32.6",
    "sequelize-cli": "^4.0.0"
